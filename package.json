{
  "name": "poe2-trade-butler",
  "version": "2.7.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && webpack --mode production && npm run script:zip",
    "build:dev": "webpack --mode development && npm run lint:dev",
    "lint": "eslint .",
    "lint:dev": "eslint . --config eslint.dev.config.mjs",
    "prettier": "prettier --write .",
    "script:generate:langs": "tsx scripts/generate-supported-languages.ts",
    "script:generate:storage-init": "tsx scripts/generate-storage-manager-initializer.ts",
    "script:zip": "tsx scripts/create-zip.ts"
  },
  "type": "module",
  "keywords": [
    "chrome-extension",
    "poe2",
    "trade",
    "sidebar",
    "history"
  ],
  "author": "NERDHEAD-lab",
  "license": "MIT",
  "description": "A Chrome Extension for POE2 Trade with a sidebar and history management.",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/archiver": "^6.0.3",
    "@types/chrome": "^0.0.293",
    "@types/node": "^24.0.3",
    "@typescript-eslint/parser": "^8.35.0",
    "archiver": "^7.0.1",
    "babel-loader": "^9.2.1",
    "chalk": "^5.4.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.2.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.4.2",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "tsx": "^4.20.3",
    "typescript-eslint": "^8.35.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2"
  }
}
